{
  "name": "rxjs-ai",
  "version": "0.1.0",
  "description": "A stream-first AI SDK built on RxJS. Framework-agnostic primitives for streaming, chat, reactive state, and UI integration.",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "vitest run",
    "playground": "npm run --prefix playground dev",
    "prepublishOnly": "npm run typecheck && npm run test && npm run build"
  },
  "keywords": [
    "rxjs",
    "ai",
    "llm",
    "streaming",
    "reactive",
    "observable",
    "chat",
    "openai",
    "anthropic",
    "state-management",
    "framework-agnostic",
    "ai-sdk"
  ],
  "author": "Harsh Patel",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/harsh071/rxjs-ai.git"
  },
  "homepage": "https://github.com/harsh071/rxjs-ai#readme",
  "bugs": {
    "url": "https://github.com/harsh071/rxjs-ai/issues"
  },
  "engines": {
    "node": ">=18"
  },
  "peerDependencies": {
    "rxjs": "^7.8.0"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^2.1.9",
    "rxjs": "^7.8.1",
    "typescript": "^5.7.3",
    "vitest": "^2.1.8"
  }
}
